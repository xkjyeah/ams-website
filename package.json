{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host 0.0.0.0",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "serve": "nuxt generate && http-server .output/public",
    "deploy": "nuxt generate && gh-pages -t -d .output/public",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "http-server": "^14.1.1",
    "nuxt": "^3.11.2",
    "nuxt-jsonld": "^2.0.8",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "gh-pages": "^6.1.1",
    "sass": "^1.77.1"
  }
}
